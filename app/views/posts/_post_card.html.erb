<div class="post_card" id="post_<%=post.id%>">
  <div class="votes">
    <%= post.votes %>
  </div>
  <p><%= post.title %></p>
  <%= image_tag post.avatar_url, alt: post.title %>
</div>

<%= image_tag "https://cdn3.iconfinder.com/data/icons/unicons-vector-icons-pack/32/plus-512.png", alt: "alttext", class: 'vote' %>

